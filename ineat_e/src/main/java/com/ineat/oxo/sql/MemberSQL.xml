<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
         PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
         "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
         
 <mapper namespace="mSQL">
 <!-- 로그인 -->
 <select id="Login" resultType="int" parameterType="mVO">
 	SELECT
 		count(*) cnt
 	FROM
 		ie_member
 	WHERE
 		m_id = #{id}
 		AND m_pw = #{pw}
 </select>
 
 
 </mapper>